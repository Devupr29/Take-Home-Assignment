{
  "name": "starter-cra-and-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "concurrently": "^7.0.0",
    "express": "^4.21.2",
    "http-proxy-middleware": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "npm run development",
    "development": "cross-env NODE_ENV=development concurrently --kill-others \"npm run client\" \"npm run server\"",
    "server": "cross-env PORT=4000 node server/server.js",
    "client": "cross-env PORT=3000 NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build": "react-scripts build",
    "production": "cross-env NODE_ENV=production npm run build && node server/server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^10.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
